CssCANNELLE : ERXStyleSheet {
	filename = "styles/cannelle.css";
	framework = "app";
}
DefaultStyles : WOString {
	value = getDefaultStyles;
	escapeHTML = false;
}


imgOffLine: WOImage {
	border = 0;
	filename = "img/offline.gif";
	framework = "FwkCktlWebApp";
}

imgItem: WOImage {
	filename = "img/item-closed.gif";
	framework = "FwkCktlWebApp";
}

linkAppURL: WOHyperlink {
	href = applicationURL;
	target = "_top";
}

siHasSiteUrlYES: WOConditional {
	condition = hasSiteURL;
}

linkSiteURL: WOHyperlink {
	href = siteURL;
	target = "_top";
}
